{
  "name": "yasp",
  "description": "Open source Dota 2 replay parsing and statistics",
  "version": "6.0.0",
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "node deploy",
    "dev": "nf -j Procfile.dev start",
    "test": "test/run.sh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "update": "npm-check-updates -u",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "mocha": "mocha",
    "maven": "mvn -q -f java_parser/pom.xml clean install -U",
    "build": "npm install && npm run maven && npm run webpack",
    "create": "bash sql/create.sh",
    "psql": "psql -d postgresql://yasp:yasp@localhost/yasp",
    "deploy": "kubectl get rc -o name --selector tier=backend | cut -d '/' -f2 | xargs -n1 kubectl rolling-update --image=yasp/yasp:`git rev-parse HEAD`"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "^1.0.7",
    "async": "1.5.2",
    "big-number": "0.3.1",
    "body-parser": "1.14.2",
    "bootstrap": "^3.3.6",
    "bootswatch": "^3.3.6",
    "bull": "yasp-dota/bull",
    "c3": "^0.4.11-rc4",
    "cal-heatmap": "^3.5.4",
    "cassandra-driver": "^3.0.0",
    "compression": "^1.6.0",
    "connect-redis": "^3.0.1",
    "cookie-session": "^2.0.0-alpha",
    "d3": "^3.5.12",
    "datatables": "^1.10.9",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "~2.0.0",
    "dota2-minimap-hero-sprites": "~1.0.1",
    "dotenv": "^2.0.0",
    "express": "~4.13.3",
    "express-session": "~1.13.0",
    "flag-icon-css": "^1.0.0",
    "font-awesome": "^4.5.0",
    "foreman": "~1.4.1",
    "heatmap.js": "~2.0.1",
    "http-proxy": "^1.12.0",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "knex": "^0.9.0",
    "moment": "^2.11.0",
    "ndjson": "^1.4.3",
    "npm-check-updates": "^2.5.6",
    "numeral": "^1.5.3",
    "passport": "0.3.2",
    "passport-steam": "1.0.1",
    "pg": "^4.4.3",
    "pg-query-stream": "^1.0.0",
    "pm2": "^1.0.0",
    "poet": "~2.0.1",
    "qtip2": "3.0.2",
    "redis": "2.4.2",
    "request": "~2.69.0",
    "request-progress": "^2.0.1",
    "selectize": "^0.12.1",
    "sentiment": "^1.0.4",
    "steam": "1.4.0",
    "stripe": "4.3.0",
    "winston": "2.1.1",
    "wordcloud": "1.0.1",
    "world-countries": "^1.7.4"
  },
  "devDependencies": {
    "coveralls": "2.11.6",
    "css-loader": "^0.23.1",
    "database-cleaner": "~0.11.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "istanbul": "~0.4.1",
    "json-loader": "^0.5.4",
    "mocha": "~2.4.5",
    "nock": "~7.0.2",
    "nodemon": "^1.8.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "engines": {
    "node": "5.4.0",
    "npm": "3.5.0"
  }
