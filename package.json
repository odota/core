{
  "name": "yasp",
  "description": "YASP: Another (Dota) Stats Page",
  "version": "3.0.0",
  "main": "web.js",
  "scripts": {
    "start": "nf -j Procfile.retriever start",
    "dev": "nf -j Procfile.dev start",
    "test": "npm run build && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "unparsed": "node runner.js unparsed",
    "constants": "node runner.js constants",
    "updateNames": "node runner.js updateNames",
    "buildSets": "node runner.js buildSets",
    "fullhistory": "node runner.js fullhistory",
    "shorthistory": "node runner.js shorthistory",
    "getmissing": "node runner.js getmissing",
    "bower": "bower install",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "build": "npm install && npm run bower && npm run constants && npm run webpack && npm run maven",
    "maven": "mvn -q -f parser/pom.xml clean install -U",
    "yasp-parser": "sudo nf export -j Procfile.parser -o /etc/init/ && sudo restart foreman",
    "yasp-core": "sudo nf export -j Procfile.prod -o /etc/init/ && sudo restart foreman"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "async": "0.9.0",
    "aws-sdk": "~2.1.23",
    "big-number": "0.3.1",
    "body-parser": "1.12.2",
    "bootstrap": "^3.3.4",
    "bower": "^1.4.1",
    "c3": "^0.4.10",
    "cal-heatmap": "^3.5.2",
    "compression": "^1.4.3",
    "connect-redis": "^2.2.0",
    "d3": "^3.5.5",
    "datatables": "^1.10.6",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "yasp-dota/node-dota2",
    "dota2-minimap-hero-sprites": "bontscho/dota2-minimap-hero-sprites",
    "dotenv": "~1.1.0",
    "express": "~4.12.3",
    "express-session": "~1.11.1",
    "font-awesome": "^4.3.0",
    "foreman": "~1.3.0",
    "heatmap.js": "pa7/heatmap.js#develop",
    "http-auth": "~2.2.5",
    "jade": "~1.9.2",
    "jquery": "^2.1.3",
    "kue": "~0.8.12",
    "moment": "^2.10.2",
    "monk": "1.0.1",
    "numeral": "^1.5.3",
    "parse-redis-url": "0.0.2",
    "passport": "0.2.1",
    "passport-steam": "0.1.4",
    "paypal-rest-sdk": "~1.5",
    "poet": "~2.0.0",
    "qtip2": "^2.2.0",
    "redis": "0.12.1",
    "request": "~2.55.0",
    "request-progress": "^0.3.1",
    "seaport": "^2.0.9",
    "select2": "^3.5.2-browserify",
    "sentiment": "^1.0.2",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "steam": "0.6.8",
    "winston": "1.0.0"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "css-loader": "^0.11.0",
    "database-cleaner": "~0.9.1",
    "file-loader": "^0.8.1",
    "istanbul": "~0.3.13",
    "mocha": "~2.2.4",
    "mongodb": "~2.0.27",
    "nock": "~1.6.0",
    "nodemon": "^1.3.7",
    "style-loader": "^0.11.0",
    "supertest": "^0.15.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.9"
  }
}
