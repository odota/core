{
  "extends": "airbnb/base",
  "rules": {
    "max-len": ["error", { "code": 180, "ignoreTemplateLiterals": true, "ignoreStrings": true }],
    "no-console": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-constant-condition": 1,
    "prefer-destructuring": 1,
    "no-underscore-dangle": 0
  }
}
